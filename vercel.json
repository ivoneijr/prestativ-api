{
  "version": 2,
  "installCommand": "yarn install && yarn prisma generate --schema=./prisma/schema.prisma",
  "buildCommand": "yarn build",
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
